                <menuitem id="personal_project_supervisor" action="student_project_view_supervisor" groups="student.group_supervisor"/>
                <menuitem id="personal_event_private" action="student_event_view_personal"/>
                
        </menuitem>

        <menuitem sequence="2" id="faculty_first_level_menu" name="Faculty">

